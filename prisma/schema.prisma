generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cards {
  id                  Int      @id
  card_name           String?
  amount              Int?
  card_category       String?
  color_identity      String?
  primary_card_type   String?
  secondary_card_type String?
  mana_value          Int?
  reserve_list        Boolean?
  deck                String?
  unique_to_deck      Boolean?
}

model colour_sort {
  colour    String @id
